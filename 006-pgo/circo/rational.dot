digraph G {
  node [shape=circle fixedsize=true width=1 height=1 style=filled label="" ]
    normal [ image="images/normal.png" ]
    ghost [ image="images/ghost.png" ]
    dark [ image="images/dark.png" ]
    psychic [ image="images/psychic.png" ]
    fighting [ image="images/fighting.png" ]
  { rank=same; normal fighting }
  { rank=same; dark psychic }
  { rank=same; ghost }
  dark -> dark [ arrowhead=tee ]
  dark -> fighting [ arrowhead=tee ]
  dark -> ghost [ penwidth=4 ]
  dark -> psychic [ penwidth=4 ]
  fighting -> normal [ penwidth=4 ]
  fighting -> ghost [ arrowhead=curve ]
  fighting -> psychic [ arrowhead=tee ]
  fighting -> dark [ penwidth=4 ]
  ghost -> normal [ arrowhead=curve ]
  ghost -> ghost [ penwidth=4 ]
  ghost -> psychic [ penwidth=4 ]
  ghost -> dark [ arrowhead=tee ]
  psychic -> psychic [ arrowhead=tee ]
  psychic -> dark [ arrowhead=curve ]
  normal -> ghost [ arrowhead=curve ]
  psychic -> fighting [ penwidth=4 ]
}
