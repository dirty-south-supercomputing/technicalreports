digraph G {
  node [shape=circle fixedsize=true width=1 height=1 style=filled label="" ]
    bug [ image="images/bug.png" ]
    ground [ image="images/ground.png" ]
    poison [ image="images/poison.png" ]
    grass [ image="images/grass.png" ]
    rock [ image="images/rock.png" ]
  { rank=same; bug ground }
  { rank=same; poison grass }
  { rank=same; rock }
  poison -> poison [ arrowhead=tee ]
  poison -> ground [ arrowhead=tee ]
  poison -> rock [ arrowhead=tee ]
  poison -> grass [ penwidth=4 ]
  ground -> poison [ penwidth=4 ]
  ground -> rock [ penwidth=4 ]
  ground -> bug [ arrowhead=tee ]
  ground -> grass [ arrowhead=tee ]
  rock -> ground [ arrowhead=tee ]
  rock -> bug [ penwidth=4 ]
  bug -> poison [ arrowhead=tee ]
  bug -> grass [ penwidth=4 ]
  grass -> poison [ arrowhead=tee ]
  grass -> ground [ penwidth=4 ]
  grass -> rock [ penwidth=4 ]
  grass -> bug [ arrowhead=tee ]
  grass -> grass [ arrowhead=tee ]
}
